    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.0",
    "@types/fs-extra": "^9.0.11",
    "@types/handlebars": "^4.1.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.37",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/sinon": "^9.0.11",
    "@types/supertest": "^2.0.11",
    "@types/terminal-kit": "^1.28.2",
