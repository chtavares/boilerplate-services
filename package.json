{
  "name": "services",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "serverless offline start",
    "dev": "nodemon",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:verify": "eslint \"{src,apps,libs,test}/**/*.ts\""
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.spec.ts"
    ],
    "watch": [
      "apps",
      "config",
      "libs"
    ],
    "ext": "ts,js,json,yml,yaml",
    "delay": 2500,
    "exec": "npm run compile && npm run start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run sonarscanner",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run test:cov"
    }
  },
  "dependencies": {
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^1.0.2",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
